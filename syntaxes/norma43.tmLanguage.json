            "9": { "name": "comment" },
            "10": { "name": "constant.regexp" },
            "11": { "name": "support.function" }
          }
        },
        {
          "name": "comment.norma43",
          "match": "(--SUC--)"
        },
        {
          "name": "invalid.norma43",
          "match": "(--ERR--)"
        }
      ]
    }
